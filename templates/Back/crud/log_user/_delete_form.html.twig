<form method="post" action="{{ path('log_user_delete', {'id': log_user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ log_user.id) }}">
    <button class="btn">Delete</button>
</form>
